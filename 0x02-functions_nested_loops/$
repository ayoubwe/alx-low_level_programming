#include "main.h"

/**
 * print_alphabet_x10 - Write a function that prints 10 times the alphabet, in lowercase, followed by a new line.
 * Return: 0
 */

void print_alphabet(void)
{
	char i;
	int j;

	for (j = 0; j < 10; j++)
	{
		for (i = 'a'; i <= 'z'; i++)
			_putchar(i);
	
	_putchar('\n');
	}
return (0);
}
